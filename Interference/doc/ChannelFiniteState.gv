digraph channel_finite_state_machine {
	rankdir=LR;
	size="8,5"
	node [shape = doublecircle]; idle;
	node [shape = circle];
	idle -> busy;
 	idle -> collision;
	busy -> idle;
	collision -> idle;

	currentState -> nextState;
	currentState -> collisionState;
	collisionState -> nextState;
	nextState -> currentState;
}
